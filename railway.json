{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "backend/Dockerfile",
    "buildCommand": "docker-compose build"
  },
  "deploy": {
    "startCommand": "docker-compose up",
    "healthcheckPath": "/",
    "healthcheckTimeout": 300
  }
}